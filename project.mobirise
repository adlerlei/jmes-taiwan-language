          "_name": "tabs2",
          "_customHTML": "<section class=\"tabs2 tabs medicalm4_tabs2\" group=\"Tabs\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <header>Show / Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Widget\" name=\"showWidget\">\n        <input type=\"checkbox\" title=\"Tab Image\" name=\"showTabImage\" checked>\n        <input type=\"checkbox\" title=\"Tab Title\" name=\"showTabTitle\">\n        <input type=\"checkbox\" title=\"Tab Text\" name=\"showTabText\">\n        <input type=\"checkbox\" title=\"Tab Cards\" name=\"showTabCards\">\n        <input type=\"checkbox\" title=\"Card Icons\" name=\"swIcon\" checked condition=\"showTabCards\">\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\" checked condition=\"showTabCards\">\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showCardText\" checked condition=\"showTabCards\">\n        <header>Colors</header>\n        <input type=\"color\" title=\"Tabs Color\" name=\"tabsColor\" value=\"#28303f\">\n        <input type=\"color\" title=\"Active Tab Color\" name=\"activeTabColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Active Tab BG\" name=\"activeTabbg\" value=\"#4f87fb\">\n        <input type=\"color\" name=\"cardBg\" title=\"Card Bg\" value=\"#ffffff\" condition=\"showTabCards\">\n        <input type=\"color\" name=\"widgetBg\" title=\"Widget Bg\" value=\"#4f87fb\" condition=\"showWidget\">\n        <select title=\"Tabs Count\" name=\"bootstrapTabs\">\n            <option